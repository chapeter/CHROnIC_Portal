{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "joshand/chronic_portal:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/imapex/chronic/portal",
    "instances": 1,
    "cpus": 0.1,
    "mem": 16,
    "env": {
        "CHRONICPORTAL": "http://imapex-chronic-portal.app.mantldevnetsandbox.com",
        "CHRONICBUS": "http://imapex-chronic-bus.app.mantldevnetsandbox.com",
        "CHRONICUCS": "http://imapex-chronic-ucs-esx-analyzer.app.mantldevnetsandbox.com"
    }
}
